create table dbo.BankHoliday
(
	HolidayDate date NOT NULL,
	CountryId smallint NOT NULL,
	Holiday varchar(50) NOT NULL,
	HolidayType varchar(50) NULL,
	IsBankHoliday  AS (case when HolidayType='Bank Holiday' then (1) else (0) end) persisted not null,
	constraint PK_BankHoliday primary key clustered (HolidayDate, CountryId, Holiday)
) 
Go
